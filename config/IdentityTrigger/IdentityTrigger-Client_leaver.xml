<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE IdentityTrigger PUBLIC "sailpoint.dtd" "sailpoint.dtd">
<IdentityTrigger disabled="true" handler="sailpoint.api.WorkflowTriggerHandler" name="Client_Leaver" type="Rule">
